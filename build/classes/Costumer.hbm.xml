<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="co.com.biblio.vo.Customer" table="costumer">
		<id name = "idcostumer" column = "idcostumer">
         <generator class="native"/>
      	</id>
      	
		<property name="tyId" column="type_identification" />
		<property name="identification" column="identification" />
		<property name="firstName" column="first_name"/>
		<property name="lastName" column="last_name"/>
		<property name="phone" column="phone"/>
		<property name="address" column="address"/>
		<property name="birthDate" column="birth_date"/>
		<property name="email" column="email"/>
		<property name="idUser" column="idUser"/>
		<property name="idadvisor" column="idadvisor"/>
		<property name="idadministrator" column="idadministrator"/>
	
		
	
	</class>
</hibernate-mapping>